
<div id="pending" class="template_div">
	<div class="page_title_div">
		<span id="account_name_<%= @account.id %>" class="account_name">
			<%= @account.name%> <%= render :partial => 'options', :locals => {:acc => @account} %>
		</span>
		<%= render :partial => 'stateli/date_span_selector_pending', :locals => {:form_url => account_pending_path(@account), :start_text_field_id => "start_date", :end_text_field_id => "end_date"}; %>
	</div>
	<%= render :partial => 'transactions/transactions_div', :locals => {:transactions => @transactions} %>
</div>